@model List<AdoDemo.Models.Employee>

<h2>Employee List</h2>

<table border="1" cellpadding="5">
    <thead>
        <tr><th>Id</th><th>Name</th><th>Email</th></tr>
    </thead>
    <tbody>
        @foreach(var e in Model)
        {
            <tr>
                <td>@e.Id</td>
                <td>@e.Name</td>
                <td>@e.Email</td>
            </tr>
        }
    </tbody>
</table>

<p>Last fetch time: @Context.Session.GetString("LastFetchTime")</p>
